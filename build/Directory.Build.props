<Project>
  <Import Project="dependencies.props" />
  <Import Project="version.props" />

  <PropertyGroup>
    <!-- Set versions for all projects. Avoids warnings from non-preview projects referencing preview projects -->
    <Version>$(CloudStateCSharpVersion)</Version>
    <AssemblyVersion>$(CloudStateCSharpVersion)</AssemblyVersion>
    <FileVersion>$(CloudStateCSharpVersion)</FileVersion>
    <!-- <TreatWarningsAsErrors>true</TreatWarningsAsErrors> -->
    <!-- <WarningsNotAsErrors>$(WarningsNotAsErrors);CS1591;</WarningsNotAsErrors> -->
    <LangVersion>8.0</LangVersion>    
    <Nullable>enable</Nullable>
    
    <PROTOBUF_SOURCE>true</PROTOBUF_SOURCE>
    <GRPC_SOURCE>false</GRPC_SOURCE>

  </PropertyGroup>

</Project>